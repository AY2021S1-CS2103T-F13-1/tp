@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":InventoryParser" as InventoryParser LOGIC_COLOR
participant "u:UndoCommand" as UndoCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":VersionedInventoryinator" as VersionedInventoryinator MODEL_COLOR
end box
[-> LogicManager : execute(undo)
activate LogicManager

LogicManager -> InventoryParser : parseCommand(undo)
activate InventoryParser

create UndoCommand
InventoryParser -> UndoCommand
activate UndoCommand

UndoCommand --> InventoryParser
deactivate UndoCommand

InventoryParser --> LogicManager : u
deactivate InventoryParser

LogicManager -> UndoCommand : execute()
activate UndoCommand

UndoCommand -> Model : undoInventoryinator()
activate Model

Model -> VersionedInventoryinator : undo()
activate VersionedInventoryinator

VersionedInventoryinator -> VersionedInventoryinator :resetData(ReadOnlyInventoryinator)
VersionedInventoryinator --> Model :
deactivate VersionedInventoryinator

Model --> UndoCommand
deactivate Model

UndoCommand --> LogicManager : result
deactivate UndoCommand
UndoCommand -[hidden]-> LogicManager : result
destroy UndoCommand

[<--LogicManager
deactivate LogicManager
@enduml
